@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@base <http://tib.de/SDM-RDFizer/mapping#> .

<#TriplesMap1>	
	rml:logicalSource [ rml:source "/data/data.csv";
						rml:referenceFormulation ql:CSV 
						];
	rr:subjectMap [
		rr:template "http://project-iasis.eu/Transcript/{Accession Number}";
	];
	rr:predicateObjectMap [
		rr:predicate iasis:transcript_isRelatedTo_gene;
		rr:objectMap [
			rml:reference "Gene name"
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:gene_isRelatedTo_sample;
		rr:objectMap [ 
			rml:reference "ID_sample" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:sample_isTakenFrom_tumor;
		rr:objectMap [ 
			rml:reference "ID_tumour" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:tumor_isTakenFrom_Tissue;
		rr:objectMap [
			rml:reference "Primary site" 
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:tissue_hasType;
		rr:objectMap [
			rml:reference "Primary histology"
		]
	];
	rr:predicateObjectMap [
		rr:predicate iasis:mentionedIn_publication;
		rr:objectMap [ 
			rml:reference "Pubmed_PMID"
		]
	];		
	rr:predicateObjectMap [
		rr:predicate iasis:isRelatedTo;
		rr:objectMap [ 
			rml:reference "Mutation genome position"
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:hasVariation;
		rr:objectMap [ 
			rml:reference "MUTATION_ID"
		]
	];	
	rr:predicateObjectMap [
		rr:predicate iasis:hasCDS;
		rr:objectMap [ 
			rrml:reference "cFormat"
		]
	].		


